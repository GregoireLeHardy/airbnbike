<h1>Bikes#index</h1>
<p>Find me in app/views/bikes/index.html.erb</p>
<h2><% @bikes.each do |bike| %></h2>
  <p><%= bike.name %></p>
  <p><%= link_to "Edit", edit_bike_path(bike) %></p>
<% end %>

<div
  id="map"
  style="width: 100%;
  height: 600px; padding-top: 50px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
