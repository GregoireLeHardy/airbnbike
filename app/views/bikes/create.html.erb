<h1>Bikes#create</h1>
<p>Find me in app/views/bikes/create.html.erb</p>

<form action="/bikes_create_path" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="name">Name: </label>
  <input type="text" name="bike[name]" id="name">

  <label for="address">Address: </label>
  <input type="text" name="bike[address]" id="address">

  <input type="submit">
</form>
